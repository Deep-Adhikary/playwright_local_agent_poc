{
    "playwright_mcp_tools": {
        "version": "1.0.0",
        "description": "Canonical list of all Playwright MCP tools for browser automation",
        "tools": [
            {
                "tool_id": "browser_click",
                "title": "Click",
                "description": "Perform click on a web page",
                "mcp": {
                    "server": "playwright",
                    "method": "browser_click"
                },
                "input_schema": {
                    "type": "object",
                    "additionalProperties": false,
                    "required": ["element", "ref"],
                    "properties": {
                        "element": {
                            "type": "string",
                            "description": "Human-readable element description used to obtain permission to interact with the element"
                        },
                        "ref": {
                            "type": "string",
                            "description": "Exact target element reference from the page snapshot"
                        },
                        "button": {
                            "type": "string",
                            "enum": ["left", "right", "middle"],
                            "default": "left",
                            "description": "Button to click"
                        },
                        "doubleClick": {
                            "type": "boolean",
                            "description": "Whether to perform a double click instead of a single click"
                        },
                        "modifiers": {
                            "type": "array",
                            "items": {"enum": ["Alt", "Control", "ControlOrMeta", "Meta", "Shift"], "type": "string"},
                            "description": "Modifier keys to press"
                        }
                    }
                },
                "output_schema": {
                    "type": "string",
                    "description": "Resulting page state after the click action"
                },
                "tags": ["ui", "interaction", "click"],
                "aliases": ["tap", "press"],
                "read_only": false
            },
            {
                "tool_id": "browser_close",
                "title": "Close Page",
                "description": "Close the browser page",
                "mcp": {
                    "server": "playwright",
                    "method": "browser_close"
                },
                "input_schema": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {}
                },
                "output_schema": {
                    "type": "string",
                    "description": "Confirmation of page closure"
                },
                "tags": ["browser", "lifecycle", "close"],
                "aliases": ["quit", "exit"],
                "read_only": false
            },
            {
                "tool_id": "browser_console_messages",
                "title": "Console Messages",
                "description": "Returns all console messages from the browser",
                "mcp": {
                    "server": "playwright",
                    "method": "browser_console_messages"
                },
                "input_schema": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "level": {
                            "type": "string",
                            "enum": ["error", "warning", "info", "debug"],
                            "default": "info",
                            "description": "Level of console messages to return"
                        }
                    }
                },
                "output_schema": {
                    "type": "array",
                    "description": "List of console messages with their levels and content"
                },
                "tags": ["debugging", "console", "logs"],
                "aliases": ["logs", "debug"],
                "read_only": true
            },
            {
                "tool_id": "browser_drag",
                "title": "Drag and Drop",
                "description": "Perform drag and drop between two elements",
                "mcp": {
                    "server": "playwright",
                    "method": "browser_drag"
                },
                "input_schema": {
                    "type": "object",
                    "additionalProperties": false,
                    "required": ["startElement", "startRef", "endElement", "endRef"],
                    "properties": {
                        "startElement": {"type": "string", "description": "Human-readable source element description"},
                        "startRef": {
                            "type": "string",
                            "description": "Exact source element reference from the page snapshot"
                        },
                        "endElement": {"type": "string", "description": "Human-readable target element description"},
                        "endRef": {
                            "type": "string",
                            "description": "Exact target element reference from the page snapshot"
                        }
                    }
                },
                "output_schema": {
                    "type": "string",
                    "description": "Result of the drag and drop operation"
                },
                "tags": ["ui", "interaction", "drag", "drop", "drag and drop"],
                "aliases": ["move", "transfer"],
                "read_only": false
            },
            {
                "tool_id": "browser_evaluate",
                "title": "Evaluate JavaScript",
                "description": "Evaluate JavaScript expression on page or element",
                "mcp": {
                    "server": "playwright",
                    "method": "browser_evaluate"
                },
                "input_schema": {
                    "type": "object",
                    "additionalProperties": false,
                    "required": ["function"],
                    "properties": {
                        "function": {
                            "type": "string",
                            "description": "JavaScript function to execute: () => { /* code */ } or (element) => { /* code */ }"
                        },
                        "element": {"type": "string", "description": "Human-readable element description (optional)"},
                        "ref": {
                            "type": "string",
                            "description": "Exact target element reference (required if element provided)"
                        }
                    }
                },
                "output_schema": {
                    "type": "any",
                    "description": "Result of the JavaScript evaluation"
                },
                "tags": ["javascript", "execution", "evaluate", "run script", "run js", "run javascript"],
                "aliases": ["execute", "run_js"],
                "read_only": false
            },
            {
                "tool_id": "browser_file_upload",
