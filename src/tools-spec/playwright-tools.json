            {
                "tool_id": "browser_run_code",
                "title": "Run Playwright Code",
                "description": "Run Playwright code snippet",
                "mcp": {
                    "server": "playwright",
                    "method": "browser_run_code"
                },
                "input_schema": {
                    "type": "object",
                    "additionalProperties": false,
                    "required": ["code"],
                    "properties": {
                        "code": {
                            "type": "string",
                            "description": "A JavaScript function containing Playwright code to execute. It will be invoked with a single argument, page, which you can use for any page interaction. For example: `async (page) => { await page.getByRole('button', { name: 'Submit' }).click(); return await page.title(); }`"
                        }
                    }
                },
                "output_schema": {
                    "type": "any",
                    "description": "Result of the Playwright code execution"
                },
                "tags": ["playwright", "code", "execution"],
                "aliases": ["execute_playwright", "run_script"],
                "read_only": false
            },
